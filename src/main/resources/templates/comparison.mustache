<html xmlns:th="http://www.thymeleaf.org">
<head>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="/select2/dist/css/select2.min.css">
    <script src="/jquery.min.js"></script>
    <script src="/select2/dist/js/select2.min.js"></script>
    <script src="/select2/dist/js/i18n/ru.js"></script>

    <script>
    $(document).ready(function() {
        $('.js-select2').select2({
            placeholder: "Выберите город",
            maximumSelectionLength: 2,
            language: "ru"
        });
    });
    </script>

    <link rel="stylesheet" type="text/css" href="styles/css/style.css" >
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
</head>
<body>
<div class="row">
    <a class="bot2 col-md" href="/institutionsRedirect"><h2 class="buttonTitle">Спорткомплексы</h2></a>
    <a class="bot2 col-md" href="/racesRedirect"><h2 class="buttonTitle">Соревнования</h2></a>
    <a class="bot2 col-md" href="/regionsRedirect"><h2 class="buttonTitle">Регионы</h2></a>
    <a class="bot2 col-md" href="/coachesRedirect"><h2 class="buttonTitle">Тренеры</h2></a>
    <a class="bot2 col-md" href="/sportsmansRedirect"><h2 class="buttonTitle">Спортсмены</h2></a>
    <a class="bot2 col-md" href="/mainRedirect"><h2 class="buttonTitle">Главная</h2></a>
    <a href="/registrationRedirect" ><img
            alt=""
            class="bot2  col-md"

            src="https://static.overlay-tech.com/assets/8d69822d-f599-4e0a-9a18-48e1afc7a1a0.svg"
    /></a>
</div>

<div class="component-2-sp">
    <div class="flex-wrapper-two-sp">
        <div class="picture">
            <div class="picture-big">
                <div>
                    <div class="icon-profile">
                        <img
                                alt=""
                                class="mask"
                                src="https://static.overlay-tech.com/assets/314c4c35-6cfd-401f-8163-6d9c4a8d9671.svg"
                        />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="flex-wrapper-two-sp2">
        <div class="picture">
            <div class="picture-big">
                <div>
                    <div class="icon-profile">
                        <img
                                alt=""
                                class="mask"
                                src="https://static.overlay-tech.com/assets/314c4c35-6cfd-401f-8163-6d9c4a8d9671.svg"
                        />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<table class="iksweb">
    <tbody>
    <tr>
        <div class="row">
            <form method="post" action="/compare">
                <td>
                    <select name="idfirstsportsman">
                        {{#sportsman}}
                            <option name="idfirstsportsman" value={{idsportsman}}>{{idsportsman}} {{name}} {{surname}}</option>
                        {{/sportsman}}
                    </select>
                </td>
                <td>
                    <div class="frame-8">
                        <div class="input">
                            <div class="button-two">
                                <button type="submit">Сравнить</button>
                            </div>
                        </div>
                    </div>
                </td>
                <td>
                    <select name="idsecondsportsman">
                        {{#sportsman}}
                            <option name="idsecondsportsman" value={{idsportsman}}>{{idsportsman}} {{name}} {{surname}}</option>
                        {{/sportsman}}
                    </select>
                </td>
            </form>
        </div>
    </tr>
    <tr>
        <div class="row">
            <td><h2 class="buttonTitle"></h2>{{#sportsman1}}{{name}} {{surname}}{{/sportsman1}}</td>
            <td><h2 class="buttonTitle"></h2>Сравнение</td>
            <td><h2 class="buttonTitle"></h2>{{#sportsman2}}{{name}} {{surname}}{{/sportsman2}}</td>
        </div>
    </tr>
        <tr>
            <div class="row">
                <td>
                    {{#sportsmanStat1}}
                    <a >Общая точность: {{accuracy}}%</a>
                    {{/sportsmanStat1}}
                </td>
                <td>
                    {{#comparison}}
                        <a >{{accuracy}}</a>
                    {{/comparison}}
                </td>
                <td>
                    {{#sportsmanStat2}}
                        <a >Общая точность: {{accuracy}}%</a>
                    {{/sportsmanStat2}}
                </td>
            </div>
        </tr>
        <tr>
            <div class="row">
                <td>
                    {{#sportsmanStat1}}
                        <a >Точность на стойке: {{standingAccuracy}}%</a>
                    {{/sportsmanStat1}}
                </td>
                <td>
                    {{#comparison}}
                        <a >{{standingAccuracy}}</a>
                    {{/comparison}}
                </td>
                <td>
                    {{#sportsmanStat2}}
                        <a >Точность на стойке: {{standingAccuracy}}%</a>
                    {{/sportsmanStat2}}
                </td>
            </div>
        </tr>
        <tr>
            <div class="row">
                <td>
                {{#sportsmanStat1}}
                    <a >Точность на лежке: {{layingAccuracy}}%</a>
                {{/sportsmanStat1}}
                </td>
                <td>
                    {{#comparison}}
                        <a >{{layingAccuracy}}</a>
                    {{/comparison}}
                </td>
                <td>
                    {{#sportsmanStat2}}
                        <a >Точность на лежке: {{layingAccuracy}}%</a>
                    {{/sportsmanStat2}}
                </td>
            </div>
        </tr>
    <tr>
        <div class="row">
            <td>
            {{#sportsmanStat1}}
                <a >{{raceNumber}}</a>
            {{/sportsmanStat1}}
            </td>
            <td>
                {{#comparison}}
                    <a >{{raceNumber}}</a>
                {{/comparison}}
            </td>
            <td>
                {{#sportsmanStat2}}
                    <a >Гонки: {{raceNumber}}</a>
                {{/sportsmanStat2}}
            </td>
        </div>
    </tr>
    </tbody>
</table>

</body>
</html>
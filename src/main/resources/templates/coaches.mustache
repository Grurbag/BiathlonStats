<html>
<head>
    <meta charset="utf-8" />
    <link rel="stylesheet" type="text/css" href="styles/css/style.css" >
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
</head>
<body>
<div class="row">
    <a class="bot2 col-md" href="/institutionsRedirect"><h2 class="buttonTitle">Спорткомплексы</h2></a>
    <a class="bot2 col-md" href="/racesRedirect"><h2 class="buttonTitle">Соревнования</h2></a>
    <a class="bot2 col-md" href="/regionsRedirect"><h2 class="buttonTitle">Регионы</h2></a>
    <a class="bot2 col-md" href="/coachesRedirect"><h2 class="buttonTitle">Тренеры</h2></a>
    <a class="bot2 col-md" href="/sportsmansRedirect"><h2 class="buttonTitle">Спортсмены</h2></a>
    <a class="bot2 col-md" href="/mainRedirect"><h2 class="buttonTitle">Главная</h2></a>
    <a href="/registrationRedirect" ><img
            alt=""
            class="bot2  col-md"

            src="https://static.overlay-tech.com/assets/8d69822d-f599-4e0a-9a18-48e1afc7a1a0.svg"
    /></a>
</div>

<div class="flex-wrapper-one">
    <a class="regiony">Тренеры</a>
    <div class="input">
        <div class="dropdown">
            <p>Сортировка по</p>
            <select class="dropdown" name="language" id="language">
                <option value="name">Названию(А-Я)</option>
                <option value="nameNo">Названию(Я-А)</option>
            </select>
        </div>
        <div class="search">
            <input class="search" type="text" placeholder="Искать здесь...">
            <button type="submit"></button>
        </div>
    </div>
</div>
<table class="iksweb">
    <tbody>
    <tr>
        <td>Код</td>
        <td>Фио</td>
        <td>Регион</td>
        <td>Учреждение</td>
    </tr>
    {{#coach}}
        <tr>

            <div class="row">
                <td>
                    <a >{{idcoach}}</a>
                </td>
                <td><form method="post" action = "/goToCoach">
                    <p style="text-align: center"><button name="idcoach" value="{{idcoach}}" type="submit">{{name}}</button>
                </form>
                </td>
                <td>
                    {{region}}
                </td>
                <td>
                    {{institution}}
                </td>
            </div>
        </tr>
    {{/coach}}
    </tbody>
</table>
<div class="flex-wrapper-one">
    <table width="400">
        <tbody>
        <tr>
            <td>
                <form method="post" action="/prevPageCoach">
                    <button type="submit" class="button-two">
                        <input name="start" value="{{start}}" hidden>
                        <input name="end" value="{{end}}" hidden>
                        <h2>Предыдущая страница</h2>
                    </button>
                </form>
            </td>
            <td>
                <form method="post" action="/nextPageCoach">
                    <button type="submit" class="button-two">
                        <input name="start" value="{{start}}" hidden>
                        <input name="end" value="{{end}}" hidden>
                        <h2>Следующая страница</h2>
                    </button>
                </form>
            </td>
        </tr>
        </tbody>
    </table>
</div>
</body>
</html>